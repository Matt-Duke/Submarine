pipeline {
    agent any
   
    stages {
        stage('Build') {
            steps {
                sh 'ls -l tools/build/pi-gen'
                sh 'tools/build/pi-gen/build-docker.sh -c $PWD/tools/build/config'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
